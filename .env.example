# Firebase Configuration
# Copy this file to .env.production and fill in your actual Firebase credentials
# Get these values from: https://console.firebase.google.com/project/YOUR_PROJECT/settings/general

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# EmailJS Configuration
# Get these values from: https://dashboard.emailjs.com/admin
# 1. Create account at https://www.emailjs.com/
# 2. Add email service (Gmail, Outlook, etc.)
# 3. Create 2 email templates (customer confirmation + admin notification)
# 4. Copy Service ID, Template IDs, and Public Key here

VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_CUSTOMER_TEMPLATE_ID=your_customer_template_id
VITE_EMAILJS_ADMIN_TEMPLATE_ID=your_admin_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key
VITE_ADMIN_EMAIL=admin@sarenacarolija.com

# Environment
VITE_ENV=production
